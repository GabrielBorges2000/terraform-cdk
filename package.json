{
  "name": "terraform-cdk",
  "version": "1.0.0",
  "main": "src/server.ts",
  "types": "src/server.ts",
  "license": "MPL-2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm env:load tsx watch src/server.ts",
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "db:migrate": "pnpm env:load prisma migrate dev",
    "db:reset": "pnpm env:load prisma migrate reset && pnpm run db:seed",
    "db:generate": "pnpm env:load prisma generate",
    "db:deploy": "pnpm env:load prisma migrate deploy",
    "db:push": "pnpm env:load prisma db push",
    "db:validate": "pnpm env:load prisma validate",
    "db:studio": "pnpm env:load prisma studio",
    "db:seed": "pnpm env:load tsx prisma/seed.ts",
    "lint": "exec biome lint --write",
    "format": "exec biome format --write",
    "check": "pnpm exec biome check --write",
    "env:load": "dotenv -e .env --"
  },
  "engines": {
    "node": ">=20.9"
  },
  "dependencies": {
    "@cdktf/provider-docker": "12.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.15.0",
    "@scalar/fastify-api-reference": "^1.34.6",
    "@types/express": "^4.17.20",
    "bcryptjs": "^3.0.2",
    "cdktf": "^0.21.0",
    "cdktf-cli": "^0.21.0",
    "constructs": "^10.4.2",
    "dayjs": "^1.11.15",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^5.0.3",
    "tsx": "^4.20.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^24.3.0",
    "prisma": "^6.15.0",
    "tsup": "^8.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}