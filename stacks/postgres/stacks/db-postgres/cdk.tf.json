{
  "//": {
    "metadata": {
      "backend": "local",
      "stackName": "db-postgres",
      "version": "0.21.0"
    },
    "outputs": {
    }
  },
  "provider": {
    "docker": [
      {
      }
    ]
  },
  "resource": {
    "docker_container": {
      "postgresContainer-postgres": {
        "//": {
          "metadata": {
            "path": "db-postgres/postgresContainer-postgres",
            "uniqueId": "postgresContainer-postgres"
          }
        },
        "env": [
          "POSTGRES_USER=docker",
          "POSTGRES_PASSWORD=docker",
          "POSTGRES_DB=cdktf-stacks"
        ],
        "image": "${docker_image.PostgresImage-postgres.name}",
        "name": "postgres-postgres",
        "ports": [
          {
            "external": 5432,
            "internal": 5432
          }
        ],
        "volumes": [
          {
            "container_path": "/var/lib/postgresql/data",
            "volume_name": "${docker_volume.pgdata-postgres.name}"
          }
        ]
      }
    },
    "docker_image": {
      "PostgresImage-postgres": {
        "//": {
          "metadata": {
            "path": "db-postgres/PostgresImage-postgres",
            "uniqueId": "PostgresImage-postgres"
          }
        },
        "keep_locally": true,
        "name": "postgres:latest"
      }
    },
    "docker_volume": {
      "pgdata-postgres": {
        "//": {
          "metadata": {
            "path": "db-postgres/pgdata-postgres",
            "uniqueId": "pgdata-postgres"
          }
        },
        "name": "pgdata_postgres"
      }
    }
  },
  "terraform": {
    "backend": {
      "local": {
        "path": "/home/codeborges/Documentos/github/terraform-cdk/terraform.db-postgres.tfstate"
      }
    },
    "required_providers": {
      "docker": {
        "source": "kreuzwerker/docker",
        "version": "3.6.2"
      }
    }
  }
}