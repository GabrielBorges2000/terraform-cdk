{
  "//": {
    "metadata": {
      "backend": "local",
      "stackName": "db-cliente2",
      "version": "0.21.0"
    },
    "outputs": {
    }
  },
  "provider": {
    "docker": [
      {
      }
    ]
  },
  "resource": {
    "docker_container": {
      "postgresContainer-cliente2": {
        "//": {
          "metadata": {
            "path": "db-cliente2/postgresContainer-cliente2",
            "uniqueId": "postgresContainer-cliente2"
          }
        },
        "env": [
          "POSTGRES_USER=cliente2",
          "POSTGRES_PASSWORD=docker",
          "POSTGRES_DB=docker"
        ],
        "image": "${docker_image.PostgresImage-cliente2.name}",
        "name": "postgres-cliente2",
        "ports": [
          {
            "external": 8002,
            "internal": 5432
          }
        ],
        "volumes": [
          {
            "container_path": "/var/lib/postgresql/data",
            "volume_name": "${docker_volume.pgdata-cliente2.name}"
          }
        ]
      }
    },
    "docker_image": {
      "PostgresImage-cliente2": {
        "//": {
          "metadata": {
            "path": "db-cliente2/PostgresImage-cliente2",
            "uniqueId": "PostgresImage-cliente2"
          }
        },
        "keep_locally": true,
        "name": "postgres:latest"
      }
    },
    "docker_volume": {
      "pgdata-cliente2": {
        "//": {
          "metadata": {
            "path": "db-cliente2/pgdata-cliente2",
            "uniqueId": "pgdata-cliente2"
          }
        },
        "name": "pgdata_cliente2"
      }
    }
  },
  "terraform": {
    "backend": {
      "local": {
        "path": "/home/codeborges/Documentos/github/terraform-cdk/terraform.db-cliente2.tfstate"
      }
    },
    "required_providers": {
      "docker": {
        "source": "kreuzwerker/docker",
        "version": "3.6.2"
      }
    }
  }
}